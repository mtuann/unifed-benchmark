{
    "framework": "fedml",
    "dataset": "cifar10",
    "algorithm": "fedavg",
    "model": "resnet56",
    "bench_param": {
        "mode": "local",
        "device": "gpu"
    },
    "training_param": {
        "epochs": 100,
        "batch_size": 128,
        "inner_step": 1,
        "tot_client_num": 10,
        "client_per_round": 5,
        "learning_rate": 0.01,
        "loss_func": "cross_entropy",
        "optimizer": "sgd",
        "optimizer_param": {
            "momentum": 0.9,
            "dampening": 0,
            "weight_decay": 0,
            "nesterov": false
        }
    }
}