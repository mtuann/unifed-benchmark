{
    "framework": "fedml",
    "dataset": "vehicle_scale_horizontal",
    "algorithm": "fed_avg",
    "model": "mlp_128"
    "bench_param": {
        "mode": "local",
        "device": "gpu"
    },
    "training_param": {
        "epochs": 1000,
        "batch_size": 32,
        "inner_step": 20,
        "client_per_round": 2,
        "tot_client_num": 2,
        "learning_rate": 0.1,
        "loss_func": "cross_entropy",
        "optimizer": "sgd",
        "optimizer_param": {
            "weight_decay": 0
        }
    }
}
